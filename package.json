{
  "name": "notarium",
  "version": "2.0.0",
  "description": "Student note-sharing platform - Youware-free edition",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "wrangler dev",
    "dev:frontend": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "npm run deploy:backend",
    "deploy:backend": "wrangler deploy",
    "deploy:frontend": "vercel --prod",
    "db:local": "wrangler d1 execute notarium-db --local --file=./database_export_20251030_121546/database_export_bf7caae7-bffe-49ae-ae17-4b0184392cc4.sql",
    "db:prod": "wrangler d1 execute notarium-db --file=./database_export_20251030_121546/database_export_bf7caae7-bffe-49ae-ae17-4b0184392cc4.sql",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.8.0",
    "@react-oauth/google": "^0.12.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251014.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "hono": "^4.10.4",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "wrangler": "^4.45.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
